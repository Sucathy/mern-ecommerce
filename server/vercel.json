{
    "installCommand": "npm intsall --legacy-peer-deps",
    "version":2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@verce/static"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}